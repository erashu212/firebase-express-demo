{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "**",
        "function": "expressApp"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      },
      {
        "source": "**/*.@(jpg|jpeg|gif|png)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=7200"
          }
        ]
      },
      {
        "source": "**/*.@(jpg|jpeg)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/jpeg"
          }
        ]
      },
      {
        "source": "**/*.@(png|PNG)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/png"
          }
        ]
      },
      {
        "source": "**/*.@(ico)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/ico"
          }
        ]
      },
      {
        "source": "**/*.@(css)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/css"
          }
        ]
      },
      {
        "source": "**/*.@(js)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/javascript"
          }
        ]
      },
      {
        "source": "**/*",
        "headers": [
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          }
        ]
      }
    ]
  },
  "functions": {
    "source": "functions"
  }
}
